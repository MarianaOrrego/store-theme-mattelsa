        "props": {
            "blockClass": "text__encuentra"
        }
    },
    "image#tienda": {
        "title": "Imagen tienda",
        "props": {
            "src": "assets/img/Tienda-ico.svg"
        }
    },
    "rich-text#encuentra": {
        "title": "Encuentra tu tienda",
        "props": {
            "text": "Encuentra tu tienda m치s cercana",
            "font": "t-small",
            "blockClass":"text__encuentra"
        }
    }
}