    "flex-layout.col#disponibilidad__tienda--product": {
        "title": "Producto elegido",
        "children": [
            "product-images",
            "vtex.store-components:product-name",
            "product-price"
        ],
        "props": {
            "width":"30%"
        }
    },
    "flex-layout.col#disponibilidad__tienda--ubication": {
        "title": "Secci칩n para ubicaci칩n en tienda e informaci칩n",
        "children": [
            "flex-layout.row#disponibilidad__tienda--menu",
            "flex-layout.row#disponibilidad__tienda--img",
            "flex-layout.row#disponibilidad__tienda--txt"
        ],
        "props": {
            "width":"70%",
            "paddingLeft": 5
        }
    },
    "flex-layout.row#disponibilidad__tienda--menu": {
        "title": "Contenido para seleccionar tienda",
        "children": [
            "vtex.menu@2.x:menu#tiendas__nacionales"
        ]
    },
    "flex-layout.row#disponibilidad__tienda--img": {
        "title": "Contenido para seleccionar tienda",
        "children": [
            "image#tienda"
        ],
        "props": {
            "blockClass": "dipo__tienda--img"
        }
    },
    "flex-layout.row#disponibilidad__tienda--txt": {
        "title": "Texto encuentra tu tienda",
