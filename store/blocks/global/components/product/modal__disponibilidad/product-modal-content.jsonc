{
    "modal-content#disponibilidad__tienda": {
        "title": "Contenido modal disponibilidad",
        "children": [
            "flex-layout.row#disponibilidad__tienda"
        ],
        "props":{
            "blockClass":"modal__disponibilidad"
        }
    },
    "flex-layout.row#disponibilidad__tienda": {
        "title": "Contenido modal disponibilidad",
        "children": [
            "flex-layout.col#disponibilidad__tienda--product",
            "flex-layout.col#disponibilidad__tienda--ubication"
        ],
        "props": {
            "colSizing": "auto",
            "blockClass": "dispo__tienda",
            "preventVerticalStretch": true
        }
